{
    "manifest_version": 3,
    "name": "Simple Search Automation",
    "version": "1.0",
    "description": "Automates searching for text in the search bar.",
    "permissions": [
      "activeTab",
      "tabs"
    ],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
      }
    },
    "background": {
      "service_worker": "background.js"
    }
  }
  